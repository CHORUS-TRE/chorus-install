daemon.jwt.secret: ${daemon_jwt_secret}
daemon.metrics.authentication.enabled: ${daemon_metrics_authentication_enabled}
daemon.metrics.authentication.username: ${daemon_metrics_authentication_username}
daemon.metrics.authentication.password: ${daemon_metrics_authentication_password}
daemon.private_key: |
  ${daemon_private_key}
storage.datastores.chorus.password: ${storage_datastores_chorus_password}

clients:
%{ if backend_s3_enabled ~}
  minio_client:
    endpoint: ${juicefs_endpoint}
    access_key_id: ${s3_access_key}
    secret_access_key: ${s3_secret_key}
    bucket_name: ${s3_bucket_name}
%{ endif ~}
  k8s_client:
    is_watcher: ${k8s_client_is_watcher}
    api_server: ${k8s_client_api_server}
    image_pull_secrets:
%{ for s in k8s_client_image_pull_secrets ~}
    - registry: ${s.registry}
      username: ${s.username}
      password: ${s.password}
%{ endfor ~}

services.authentication_service.modes.keycloak.openid.client_secret: ${keycloak_openid_client_secret}
services.steward.user.password: ${steward_user_password}